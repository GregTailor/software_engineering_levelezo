<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="hu.unideb.inf.NewGameController"
      alignment="center">
    <children>
        <Label text="Sodoku" />
        <Button id="easyGameButton" text="Easy" onAction="#easyGame"/>
        <Button id="mediumGameButton" text="Medium" onAction="#mediumGame"/>
        <Button id="hardGameButton" text="Hard" onAction="#hardGame"/>
        <Button id="backButton" text="Back" onAction="#back"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
